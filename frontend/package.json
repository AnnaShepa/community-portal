{
  "name": "test-community-portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",
    "@types/lodash": "^4.14.111",
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "react-test-renderer": "^16.4.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^2.0.4",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "PORT=8080 react-scripts-ts start",
    "dev": "nodemon -w ./ -x npm run start -e ts,tsx",
    "deploy": "npm run build && aws s3 sync build/* s3://opensource.engcom.magento.com",
    "build": "react-scripts-ts build",
    "test": "jest",
    "eject": "react-scripts-ts eject",
    "build:webpack": "webpack"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/fetch-mock": "^6.0.3",
    "@types/jest": "^23.0.2",
    "@types/material-ui": "^0.20.8",
    "@types/node": "^10.3.2",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/recompose": "^0.26.1",
    "@types/redux-mock-store": "0.0.13",
    "@types/uuid": "^3.4.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-jest-hoist": "^23.2.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "fetch-mock": "^6.4.4",
    "file-loader": "^1.1.11",
    "jest": "^23.1.0",
    "jest-css-modules-transform": "^2.0.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-svg-transformer": "^1.0.0",
    "jsonwebtoken": "^8.3.0",
    "nodemon": "^1.17.5",
    "react-svg-loader": "^2.1.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "svg-react-loader": "^0.4.5",
    "ts-jest": "^22.4.6",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.9.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jestSetup.ts",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".+\\.(css|styl|less|sass|scss)$": "./node_modules/jest-css-modules-transform",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
    "testPathIgnorePatterns": [
      "/build/"
    ],
    "globals": {
      "API_ENDPOINT": "http://localhost:3000"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.ts",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "nodemonConfig": {
    "ignore": "test/*"
  }
}
